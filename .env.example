# Required: OpenAI API key
OPENAI_API_KEY=sk-your-key-here

# Optional: Override the model (defaults to gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=sk_your_key_here
# Optional allowlist + default voice selection (comma/newline separated)
ELEVENLABS_VOICE_IDS=
# Optional (off by default). When enabled, rejects voiceIds not in ELEVENLABS_VOICE_IDS.
ELEVENLABS_STRICT_VOICE_ALLOWLIST=0
